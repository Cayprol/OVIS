<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record model="ir.ui.view" id="inherit_view_stock_move_line_operation_tree">
			<field name="model">stock.move.line</field>
			<field name="name">inherit.stock.move.line.operations.tree</field>
			<field name="inherit_id" ref="stock.view_stock_move_line_operation_tree"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='product_uom_id']" position="after">
					<field name="g_weight" attrs="{'readonly': ['|', '|', ('is_initial_demand_editable', '=', True), '&amp;', ('state', '=', 'done'), ('is_locked', '=', True), '&amp;', ('package_level_id', '!=', False), ('parent.picking_type_entire_packs', '=', True)]}"/>

					<!-- <field name="weight_uom_id" options="{'no_open': True, 'no_create': True}" attrs="{'readonly': ['|', ('product_uom_qty', '!=', 0.0), '&amp;', ('package_level_id', '!=', False), ('parent.picking_type_entire_packs', '=', True)]}" string="Weight Unit" groups="uom.group_uom"/> -->
					<field name="weight_uom_id" options="{'no_open': True, 'no_create': True}" attrs="{'readonly': ['|', ('product_uom_qty', '!=', 0.0), '&amp;', ('package_level_id', '!=', False), ('parent.picking_type_entire_packs', '=', True)]}" string="Weight Unit"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>